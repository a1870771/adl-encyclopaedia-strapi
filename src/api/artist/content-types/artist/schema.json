{
  "kind": "collectionType",
  "collectionName": "artists",
  "info": {
    "singularName": "artist",
    "pluralName": "artists",
    "displayName": "Artist"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Bio": {
      "type": "blocks",
      "required": false
    },
    "Status": {
      "type": "boolean",
      "default": true
    },
    "Picture": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "artists"
    }
  }
}
